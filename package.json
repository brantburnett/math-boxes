{
  "name": "math-boxes",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0",
    "start:pwa": "ng build --configuration production && http-server --proxy http://localhost:8080? -p 8080 -c-1 -o math-boxes dist",
    "build": "ng build",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "deploy": "ng deploy --base-href=/math-boxes/ --name=\"brantburnett\" --email=bburnett@centeredgesoftware.com",
    "cypress:open": "concurrently \"npm run start\" \"cypress open\"",
    "cypress:run": "concurrently \"npm run start\" \"cypress run\""
  },
  "private": true,
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/src/setup-jest.ts"
    ],
    "globalSetup": "jest-preset-angular/global-setup",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/cypress/"
    ]
  },
  "dependencies": {
    "@angular/animations": "^18.2.13",
    "@angular/cdk": "^18.2.14",
    "@angular/common": "^18.2.13",
    "@angular/compiler": "^18.2.13",
    "@angular/core": "^18.2.13",
    "@angular/forms": "^18.2.13",
    "@angular/material": "^18.2.14",
    "@angular/platform-browser": "^18.2.13",
    "@angular/platform-browser-dynamic": "^18.2.13",
    "@angular/router": "^18.2.13",
    "@angular/service-worker": "^18.2.13",
    "rxjs": "^7.8.1",
    "tslib": "~2.6.2",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular-eslint/builder": "^18.4.3",
    "@angular-eslint/eslint-plugin": "^18.4.3",
    "@angular-eslint/eslint-plugin-template": "^18.4.3",
    "@angular-eslint/schematics": "^18.4.3",
    "@angular-eslint/template-parser": "^18.4.3",
    "@angular/build": "^18.2.19",
    "@angular/cli": "^18.2.19",
    "@angular/compiler-cli": "^18.2.13",
    "@angular/language-service": "^18.2.13",
    "@cypress/schematic": "^2.5.1",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.16",
    "@typescript-eslint/eslint-plugin": "^8.33.1",
    "@typescript-eslint/parser": "^8.33.1",
    "@typescript-eslint/utils": "^8.33.1",
    "angular-cli-ghpages": "^1.0.7",
    "concurrently": "^8.2.2",
    "cypress": "13.6.4",
    "eslint": "^8.57.1",
    "http-server": "^14.1.1",
    "jest": "^29.7.0",
    "jest-preset-angular": "^14.6.0",
    "ts-node": "~10.9.2",
    "typescript": "~5.5.4"
  }
}
